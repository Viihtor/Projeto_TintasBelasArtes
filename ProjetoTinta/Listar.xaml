<Window x:Class="ProjetoTinta.Listar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Listar" Height="340" Width="575">
    <Window.Background>
        <ImageBrush ImageSource="img/background_progama.jpg"/>
    </Window.Background>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <DataGrid x:Name="dgTintas" AutoGeneratedColumns="dgTintas_AutoGeneratedColumns"></DataGrid>
    </Grid>
</Window>
